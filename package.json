{
  "name": "@apexxcloud/react",
  "version": "1.0.2",
  "description": "React components for ApexxCloud Storage Service",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "style": "dist/styles.css",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "rollup -c",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@apexxcloud/sdk-js": "^1.0.18",
    "clsx": "^2.1.1",
    "react-dropzone": "^14.3.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.3.18",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "rollup": "^2.79.2",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "react",
    "apexxcloud",
    "storage",
    "upload"
  ],
  "author": "ApexxCloud <team@apexxcloud.com>",
